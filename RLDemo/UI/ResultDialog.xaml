<Window x:Class="RLDemo.UI.ResultDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ResultDialog" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        
        <WrapPanel Orientation="Horizontal" Grid.Row="0">
            <Label Content="Disziplin: "/>
            <TextBox Name="_disciplineBox" Width="200"></TextBox>
        </WrapPanel>
        
        <WrapPanel Grid.Row="1">
            <Label Content="Resultat: "></Label>
            <TextBox Name="_resultBox" Width="200"></TextBox>
        </WrapPanel>
        
        <Button Content="OK" Grid.Row="2" Click="ok_click"/>
        
    </Grid>
</Window>
